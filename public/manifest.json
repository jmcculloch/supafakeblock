{
  "manifest_version": 3,

  "key": "",

  "name": "Supafakeblock",
  "description": "Distributed blacklist for reporting and identifying Facebook scammers, spammers, fake accounts, etc.",
  "version": "0.5.1",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "content_scripts": [
      {
          "matches": ["https://www.facebook.com/*"],
          "js": ["js/vendor.js", "js/content_script.js"],
          "css": ["supafakeblock.css"]
      }
  ],

  "background": {
    "service_worker": "js/background.js",
    "scripts": ["js/background.js"]
  },

  "permissions": [
    "contextMenus", "identity", "storage", "declarativeContent"
  ],

  "browser_specific_settings": {
    "gecko": {
      "id": "supafakeblock@jmcculloch"
    }
  }
}
